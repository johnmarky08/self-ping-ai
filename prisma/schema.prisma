datasource db {
  provider = "sqlite" // or any other database provider (e.g., postgres)
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id        Int      @id @default(autoincrement())
  url       String   @unique
  createdAt DateTime @default(now())
}
